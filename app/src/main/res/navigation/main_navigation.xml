<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/main_navigation"
    app:startDestination="@id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.shoppi.februaryclfirstapp.HomeFragmentActivity"
        android:label="HomeFragmentActivity" >
        <action
            android:id="@+id/action_navigation_home_to_detailIUFragment"
            app:destination="@id/detailIUFragment" />
    </fragment>
    <fragment
        android:id="@+id/navigation_cart"
        android:name="com.shoppi.februaryclfirstapp.CartFragment"
        android:label="CartFragment" />
    <fragment
        android:id="@+id/navigation_category"
        android:name="com.shoppi.februaryclfirstapp.CategoryFragment"
        android:label="CategoryFragment" />
    <fragment
        android:id="@+id/detailIUFragment"
        android:name="com.shoppi.februaryclfirstapp.DetailIUFragment"
        android:label="DetailIUFragment" />
</navigation>